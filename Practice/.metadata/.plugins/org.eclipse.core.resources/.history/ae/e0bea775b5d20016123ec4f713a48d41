
public class PrimeNumber {
	

	public static void main(String[] args) {
	
		int count=0;
		for(int i=0;i<10;i++)
		{
			if(i%2==0)
			{
				count=count+1;
			}
			if(count==0)
			{
				System.out.println("It is not Prime Number "+i);
			}
			else
			{
				System.out.println("Prime Number "+i);
			}
		}		
	}
}