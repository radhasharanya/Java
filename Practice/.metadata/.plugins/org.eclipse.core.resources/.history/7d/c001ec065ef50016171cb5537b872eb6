public class HashMapClass {

   public static void main(String args[]) {
	   int a[] = new int[]{24,45};
	   int b[]=new int[]{16,18};
	   int blue[]= new int[]{15,12};
	   int temp;
	   for(int i =0;i<a.length;i++){
	
		   for(int j=i+1;j<a.length-1;j++)
		   {
			  
			  if(a[i]<=a[j]){
				 
				  
				  temp=a[j];
				  break;
				 
			  }
			  i=j;
			  j=j+1;
		   }
		   
	   }
	   System.out.println(temp);
			 
			 
		   }
	   
   }

