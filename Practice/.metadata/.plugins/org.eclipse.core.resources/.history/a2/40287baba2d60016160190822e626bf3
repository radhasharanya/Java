package Assignmentprograms;

public class StringMethods {

	public static void main(String[] args) {
		String str="Computer programing";
		String str1="computer Programing";
		System.out.println("String Equal ignore case "+str.equalsIgnoreCase(str1));
		System.out.println("String ChatAt "+str.charAt(15));
		System.out.println("String Substring "+str.substring(9, 19));
		System.out.println("String COmparision "+str.length());
		
		

	}

}
