
public class PrimeNumber {

	public static void main(String[] args) {

		int count = 0;
		for (int i = 1; i < 10; i++) {
			for (int j = 1; j < =i; j++) {

				if (i % j == 0) {
					count++;
				}
				System.out.println(i);
				System.out.println("j="+j);

			}
			if(count==2)
			{
				System.out.println("Prime Number"+i);
			}
			

		}

	}
}